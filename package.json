{
  "name": "opendsu-sdk",
  "version": "1.0.0",
  "description": "This repo is meant to be used for opendsu sdk development.",
  "scripts": {
    "dev-install": "npm install --dev && npm run postDevinstall",
    "postDevinstall": "node ./node_modules/octopus/scripts/run dependencies devmode",
    "build": "node ./bin/scripts/build.js --projectMap=./build/build.json --output=./build/output/",
    "apihub": "node ./node_modules/octopus/scripts/setEnv --file=../../../env.json \"node bin/scripts/apiHubLauncher.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenDSU/opendsu-sdk.git"
  },
  "keywords": [
    "opendsu",
    "ssapps",
    "dsu"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenDSU/opendsu-sdk/issues"
  },
  "homepage": "https://github.com/OpenDSU/opendsu-sdk#readme",
  "devDependencies": {
    "@msgpack/msgpack": "^1.8.0",
    "browserify": "^16.5.0",
    "common-shakeify": "^1.1.1"
  },
  "dependencies": {
    "octopus": "git+http://git@github.com/privatesky/octopus.git"
  }
}
